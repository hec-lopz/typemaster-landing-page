body
  margin 0
  padding 0
html
  box-sizing border-box
*, *::after, *::before
  box-sizing inherit

figure, input, a, button, ul, li
  padding 0
  margin 0
  font-family inherit
  color inherit
button
  border none
  background none
a
  text-decoration none
ul
  list-style none

.img
  width 100%
  height @width
  object-fit contain 